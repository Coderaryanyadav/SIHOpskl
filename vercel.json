{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/workbox-(.*).js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "^/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "^/precache-manifest\\.(.*)\\.js$",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "^/logo(\\.\\w+)?\\.(png|jpe?g|gif|svg|ico)$",
      "headers": {
        "cache-control": "public, max-age=31536000, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "^/(.*)\\.(css|js|wasm|woff2|woff|ttf|eot)$",
      "headers": {
        "cache-control": "public, max-age=31536000, must-revalidate"
      },
      "continue": true
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "cleanUrls": true,
  "trailingSlash": false
}
